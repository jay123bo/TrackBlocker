{
  "manifest_version": 3,
  "name": "TrackBlocker",
  "version": "1.1",
  "permissions": ["declarativeNetRequest", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "rules.json"
    }]
  }
}
